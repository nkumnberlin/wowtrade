{
	"compilerOptions": {
		"extends": "astro/tsconfigs/base",
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"types": [
			"astro/client"
		],
		"plugins": [
			{
				"name": "@astrojs/ts-plugin"
			},
		],
		"baseUrl": ".",
		"paths": {
			"@globals": [
				"src/components/global"
			],
			"@component/*": [
				"src/components/*"
			],
			"@style/*": [
				"src/styles/*"
			],
			"@util/*": [
				"utils/*"
			],
			"@img/*": [
				"src/assets/img/*"
			],
			"@svg/*": [
				"src/assets/svg/*"
			],
			"@icon/*": [
				"src/assets/svg/icon/*"
			],
			"@/character": [
				"src/api/character/characterService"
			],
			"@/order": [
				"src/api/order/orderService"
			],
			"@/profession": [
				"src/api/profession/professionService"
			],
			"@/craftedItem": [
				"src/api/profession/craftedItemService"
			],
			"@/recipe": [
				"src/api/profession/recipeService"
			],
			"@/*": [
				"src/"
			]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowSyntheticDefaultImports": true
	}
}
